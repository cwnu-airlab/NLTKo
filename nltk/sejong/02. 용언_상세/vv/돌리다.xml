<superEntry>
	<orth>돌리다</orth>
	<entry n="1" pos="vv">
		<mnt_grp>
			<cre>
				<cre_date>2002/08/19</cre_date>
				<cre_writer>김건희</cre_writer>
			</cre>
			<mod>
				<mod_date>2002/10/15</mod_date>
				<mod_writer>허용</mod_writer>
			</mod>
			<mod>
				<mod_date>2003/07/17</mod_date>
