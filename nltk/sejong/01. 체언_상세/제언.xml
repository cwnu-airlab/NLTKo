				<sem_class>보고제안</sem_class>
				<lr>
					<syn type="etc"></syn>
					<ant type="etc"></ant>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~을 하다</form>
					<frame>X-이 Y-을 Z-에 ~을 하다</frame>
					<frame>X-이 Y-을 Z-에게 ~을 하다</frame>
					<frame>X-이 Z-에 S자고 ~을 하다</frame>
					<frame>X-이 Z-에게 S자고 ~을 하다</frame>
				</comb_v>
				<max_n_str>X-의 Y ~</max_n_str>
				<max_n_str>X-의 Z-에대한 Y ~</max_n_str>
				<max_n_str>X-의 S자는 ~</max_n_str>
				<sel_rst arg="X">인간</sel_rst>
				<sel_rst arg="Y">추상적대상|행위</sel_rst>
				<sel_rst arg="Z">인간|인간집단</sel_rst>
