				<trans>shame</trans>
				<trans>disgrace</trans>
				<domain></domain>
				<reg></reg>
				<con></con>
				<curs type="U"/>
				<sem_class>내재적심리상태</sem_class>
				<lr>
					<syn type="temp"></syn>
					<ant type="temp"></ant>
					<rel>부끄러움</rel>
