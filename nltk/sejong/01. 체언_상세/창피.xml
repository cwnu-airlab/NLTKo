					<form vcompound="no">~를 알다</form>
					<frame></frame>
				</comb_v>
				<comb_v type="npred_vpred">
				<!-- narg_vpred 수정 -->
					<form vcompound="no">~를 모르다</form>
					<frame></frame>
				</comb_v>
