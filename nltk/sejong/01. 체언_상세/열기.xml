					<ant type="temp"></ant>
					<rel></rel>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="appr">~가 뜨겁다</comb_aj>
				<comb_aj type="appr">~가 후끈하다</comb_aj>
				<comb_n type="temp"></comb_n>
				<comb_v type="npred_vpred">
