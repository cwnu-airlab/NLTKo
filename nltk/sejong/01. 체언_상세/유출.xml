				<trans>outflow</trans>
				<trans>efflux</trans>
				<domain></domain>
				<reg></reg>
				<con></con>
				<curs type="C"/>
				<sem_class>단독행위</sem_class>
				<lr>
					<syn type="etc">방출</syn>
					<syn type="etc">방류</syn>
					<ant type="etc">유입</ant>
					<hyper>행위</hyper>
					<rel></rel>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="magn">~이 많다</comb_aj>
				<comb_aj type="magn">~이 크다</comb_aj>
				<comb_aj type="magn">~이 상당하다</comb_aj>
				<comb_n type="compound">폐수의 ~</comb_n>
				<comb_n type="compound">기름의 ~</comb_n>
				<comb_n type="compound">문화재의 ~</comb_n>
				<comb_n type="compound">정보의 ~</comb_n>
				<comb_v type="narg_vpred">
