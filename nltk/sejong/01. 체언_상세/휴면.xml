				<domain>생물</domain>
				<sem_class>상태</sem_class>
				<lr>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~을 하다</form>
					<frame>X-이 ~을 하다</frame>
				</comb_v>
				<comb_v type="npred_vsupvar">
