				<trans>selection</trans>
				<domain></domain>
				<reg></reg>
				<con></con>
				<curs type="U"/>
				<sem_class>결과행위</sem_class>
				<lr>
					<syn type="temp"></syn>
					<ant type="temp"></ant>
					<coord>선출</coord>
					<coord>선발</coord>
					<coord>당첨</coord>
					<rel></rel>
				</lr>
			</sem_grp>
			<syn_grp>
				<comb_aj type="appr">~이 공정하다</comb_aj>
				<comb_aj type="appr">~이 엄격하다</comb_aj>
				<comb_n type="compound">~ 작업</comb_n>
				<comb_n type="compound">~ 과정</comb_n>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~을 하다</form>
					<frame>X-이 Y-을 ~을 하다</frame>
					<frame>X-이 Z-을 Y-로 ~을 하다</frame>
					<frame>X-이 Z-을 Y-에 ~을 하다</frame>
				</comb_v>
				<comb_v type="npred_vsup">
					<form vcompound="yes">~이 되다</form>
					<frame>Y-이 ~이 되다</frame>
					<frame>Z-이 Y-로 ~을 하다</frame>
					<frame>Z-이 Y-에 ~을 하다</frame>
				</comb_v>
				<!--vcompound: ~하다-->
				<!--vcompound: ~되다-->
				<max_n_str>X-의 Y ~</max_n_str>
				<max_n_str>X-의 Z ~</max_n_str>
				<max_n_str>Z-의 Y-로의 ~</max_n_str>
				<max_n_str>Z-의 Y-에의 ~</max_n_str>
				<max_n_str>X-의 ~</max_n_str>
				<max_n_str>Y-의 ~</max_n_str>
