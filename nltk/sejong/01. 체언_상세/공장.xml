			<syn_grp>
				<comb_aj type="temp"></comb_aj>
				<comb_n type="temp"></comb_n>
				<comb_v type="narg_vpred">
					<form vcompound="no">~이 되다</form>
					<frame></frame>
				</comb_v>
				<max_n_str></max_n_str>
				<sel_rst arg="temp"></sel_rst>
				<cl type="uni">명</cl>
				<prt></prt>
				<av></av>
				<pre_d/>
